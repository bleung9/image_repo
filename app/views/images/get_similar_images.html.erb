<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Width</th>
      <th>Height</th>
      <th>Extension</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image.image %></td>
        <td><%= image.width %></td>
        <td><%= image.height %></td>
        <td><%= image.extension %></td>
        <td><%= link_to 'Show', image %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Back', images_path %>

